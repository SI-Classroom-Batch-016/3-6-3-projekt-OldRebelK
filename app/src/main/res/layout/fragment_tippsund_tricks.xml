<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.TippsUndTricksFragment">

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:scaleType="fitXY"
        android:src="@drawable/background1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.5" />

    <ImageButton
        android:id="@+id/backBTN"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="4dp"
        android:backgroundTint="#00FFFFFF"
        android:src="@drawable/altpixelbutton4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.177"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="404dp"
        android:layout_height="703dp"
        android:layout_marginTop="52dp"
        android:scaleType="fitXY"
        android:src="@drawable/pixeltextbox2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/TuTIconIV"
        android:layout_width="63dp"
        android:layout_height="55dp"
        android:layout_marginStart="52dp"
        android:layout_marginBottom="200dp"
        android:src="@drawable/axelayicon"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tutTextTV"
        android:layout_width="335dp"
        android:layout_height="554dp"
        android:fontFamily="@font/dotgothic16"
        android:text="TextView"
        android:textColor="#FF9100"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView15"
        app:layout_constraintEnd_toEndOf="@+id/imageView15"
        app:layout_constraintHorizontal_bias="0.55"
        app:layout_constraintStart_toStartOf="@+id/imageView15"
        app:layout_constraintTop_toTopOf="@+id/imageView15"
        app:layout_constraintVertical_bias="0.201" />

    <ImageButton
        android:id="@+id/arcadeBTN"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="52dp"
        android:layout_marginBottom="4dp"
        android:backgroundTint="#00FFFFFF"
        android:src="@drawable/altpixelbutton4"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/backarrow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/asharrow"
        app:layout_constraintBottom_toBottomOf="@+id/backBTN"
        app:layout_constraintEnd_toEndOf="@+id/backBTN"
        app:layout_constraintStart_toStartOf="@+id/backBTN"
        app:layout_constraintTop_toTopOf="@+id/backBTN" />

    <ImageView
        android:id="@+id/arcadeIcon"
        android:layout_width="47dp"
        android:layout_height="53dp"
        android:src="@drawable/asharcadereturnicon"
        app:layout_constraintBottom_toBottomOf="@+id/arcadeBTN"
        app:layout_constraintEnd_toEndOf="@+id/arcadeBTN"
        app:layout_constraintStart_toStartOf="@+id/arcadeBTN"
        app:layout_constraintTop_toTopOf="@+id/arcadeBTN" />
</androidx.constraintlayout.widget.ConstraintLayout>